	<BLF_Busy_LED group="Attendant_Console/Attendant_Key_LED_Pattern"/>
	<BLF_Held_LED group="Attendant_Console/Attendant_Key_LED_Pattern"/>
	<Unit_1_Enable group="Attendant_Console/General">Yes</Unit_1_Enable>
